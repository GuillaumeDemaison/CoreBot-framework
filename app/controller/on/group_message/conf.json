{
    "controller": {
        "on": {
            "group_message": {
                "enable": true,
                "listener": {
                    "group_message": {
                        "from": "group_message",
                        "privilege": ["user"],
                        "access_list": ["default"]
                    }
                },
                "msg": {
                    "help": [
                        "Group message event\n",
                        "- _No argument_"
                    ]
                }
            }
        }
    }
}

