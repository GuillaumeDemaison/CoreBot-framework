{
    "controller": {
        "on": {
            "user_space_leave": {
                "enable": true,
                "listener": {
                    "user_space_leave": {
                        "from": "user_space_leave",
                        "privilege": ["user"],
                        "access_list": ["default"]
                    }
                },
                "msg": {
                    "help": [
                        "Event when user leaves a space\n",
                        "- _No argument_"
                    ]
                }
            }
        }
    }
}
