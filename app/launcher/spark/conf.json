            "store": "data/spark",
            "user_presence": false,
            "user_presence_delay": 10,
            "msg": {
                "help": [
                    "Cisco Webex Teams (Spark) launcher\n",
                    "- enable: true or false, active or not the launcher",
                    "- name: chatbot name",
                    "- mail: bot's email address",
                    "- botid: bot's id",
                    "- url: host url with htttp/https and port",
                    "- port: local port",
                    "- access_token: Cisco Webex Teams bot token",
                    "- secret: specific secret (ie random)",
                    "- user_presence: info on user presence for Webex",
                    "- user_presence_delay: seconds between checks of presence",
                    "- webhook_name: the name of the webhook used to registered the bot in the Webex Teams cloud"
                ]
            }
        }
